#include <stdio.h>

// 수년간의 경험을 4바이트 바이너리 문자열로 변환하는 함수
void print_binary(int num) {
(int i = 31; i >= 0; i--)에 대하여 {
printf("%d", (num >> i) & 1);
if (i % 8 == 0) printf(" "); // 가독성을 위해 8비트마다 공백 추가
}
}

void print_intro(char* 인트로) {
// 소개 부분을 인쇄합니다(마침표 뒤에 자동으로 줄바꿈)
printf("소개 | ");
int i = 0; intro[i] != '\0'; i++에 대해 {
printf("%c", intro[i]);
intro[i] == '.' && intro[i + 1] != '\0'인 경우 {
printf("\n "); // 마침표 뒤에 줄바꿈하고 들여쓰기
}
}
printf("\n===========================================================================\n");
}

